{
	"name": "OAuth",
	"author": [
		"Trevor Uehlin"
	],
	"url": "https://github.com/ocdladefense/wiki-extension-oauth.git",
	"descriptionmsg": "oAuth-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\OAuth\\": "includes/"
	},
	"config": {
		"OAuthEnableFoo": {
			"description": "Enable the Foo feature.",
			"value": true
		},
		"OAuthVandalizeEachPage": {
			"description": "Write 'OAuth was here' on each page",
			"value": false
		}
	},
	"HookHandlers": {
		"OAuthHooks": {
			"class": "MediaWiki\\Extension\\OAuth\\Hooks"
		}
	},
	"Hooks": {
		"BeforePageDisplay": "OAuthHooks"
	},
	"MessagesDirs": {
		"OAuth": [
			"i18n"
		]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "OAuth/resources"
	},
	"ResourceModules": {
		"ext.oAuth": {
			"dependencies": [],
			"messages": [],
			"styles": [],
			"scripts": []
		}
	},
	"manifest_version": 2
}
