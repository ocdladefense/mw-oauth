{
	"name": "OAuth",
	"author": [
		"Trevor Uehlin"
	],
	"url": "https://github.com/ocdladefense/wiki-extension-oauth.git",
	"descriptionmsg": "oAuth-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\OAuth\\": "includes/"
	},
	"AutoloadClasses":{
		"SpecialOAuthEndpoint":"SpecialOAuthEndpoint.php"
	},
	"config": {
		"OAuthTestConfigurationItem": {
			"description": "This is a test.",
			"value": "This is the test configuration"
		}
	},
	"HookHandlers": {
		"LinkHook": {
			"class": "MediaWiki\\Extension\\OAuth\\HookAddLink"
		}
	},
	"Hooks": {
		"PersonalUrls": "LinkHook"
	},
	"SpecialPages": {
		"OAuthEndpoint": "SpecialOAuthEndpoint"
	},
	"MessagesDirs": {
		"OAuth": [
			"i18n"
		]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "OAuth/resources"
	},
	"ResourceModules": {
		"ext.oAuth": {
			"dependencies": [],
			"messages": [],
			"styles": [],
			"scripts": []
		}
	},
	"manifest_version": 2
}
